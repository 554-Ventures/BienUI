{
  "name": "@bienui/core",
  "version": "1.0.3",
  "description": "Production-ready React component library with accessibility-first design, Radix UI primitives, and comprehensive Storybook documentation",
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "typescript",
    "radix-ui",
    "accessibility",
    "storybook"
  ],
  "homepage": "https://github.com/554-Ventures/BienUI#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/554-Ventures/BienUI.git"
  },
  "bugs": {
    "url": "https://github.com/554-Ventures/BienUI/issues"
  },
  "license": "MIT",
  "author": {
    "name": "554 Ventures",
    "url": "https://github.com/554-Ventures"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/bien-ui.cjs.js",
  "module": "./dist/bien-ui.esm.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/bien-ui.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/bien-ui.esm.js",
      "require": "./dist/bien-ui.cjs.js"
    },
    "./styles": "./dist/bien-ui.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.config.lib.ts && tsc --project tsconfig.lib.json",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "prepublishOnly": "yarn build:lib",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir storybook-static",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "version:major": "yarn version --major",
    "deploy:storybook": "yarn build-storybook && echo 'Storybook built successfully. Push to main/master to deploy to GitHub Pages.'",
    "publish:dry": "npm publish --dry-run",
    "publish:beta": "npm publish --tag beta",
    "publish:latest": "npm publish --tag latest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-hover-card": "^1.1.5",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-menubar": "^1.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.4",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "clsx": "^2.1.0",
    "lucide-react": "^0.562.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^9.0.0",
    "react-dnd-touch-backend": "^16.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-themes": "^8.6.15",
    "@storybook/react": "8.4.7",
    "@storybook/react-vite": "8.4.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser": "^3.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "glob": "^10.3.10",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "storybook": "8.4.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
